<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!--基于构造器和setter的依赖注入方式 -->
	<bean name="Product_constr" class="app16a.Product">
		<constructor-arg name="name" value="tss" />
		<constructor-arg name="description" value="HE IS A NEU STU" />
		<constructor-arg name="price" value="10.00" />
	</bean>
	<bean name="Product2_setter" class="app16a.Product">
		<property name="name" value="zmh" />
		<property name="description" value="HE IS A NEU STU" />
		<property name="price" value="11.00" />
	</bean>
	<!-- 分割 -->
</beans>
